<% include includes/head %>

<% posts.forEach(function (post, index) { %>
<p><h2><a href="/u/<%= post.username %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
<p class="info">
	作者：<a href="/u/<%= post.username %>"><%= post.username %></a> | 
	日期：<%= post.time.minute %>
</p>
<p><%- post.post %></p>
<% }) %>

<% include includes/foot %>