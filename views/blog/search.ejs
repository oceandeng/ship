<% include includes/head %>
<title><%= title %></title>
<% include includes/style %>
</head>
<body>
<header>
	<% include includes/top %>
</header>

<article class="wrap container">
	<section class="tag-box tag-list">
		<% var lastYear = 0 %>
		<% posts.forEach(function (post, index) { %>
			<% if (lastYear != post.time.year) { %>
				<h3><%= post.time.year %></h3>
			<% lastYear=post.time.year } %>
			<div class="row">
				<div class="col-xs-3">
					<time><%= post.time.day %></time>
				</div>
				<div class="col-xs-9">
					<a href="/blog/u/<%= post.username %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a>
				</div>
			</div>
		<% }) %>
	</section>
</article>

<footer></footer>

