<% include includes/head %>
<title><%= title %></title>
<% include includes/style %>
</head>
<body>
<header>
	<% include includes/top %>
</header>

<article class="wrap">
	<section class="post-box">
		<div class="post-list">
			<% posts.forEach(function(post, index) { %>
				<h2><a href="/blog/u/<%= post.username %>/"><%= post.title %></a></h2>
				<p class="info">
					作者：<a href="/blog/u/<%= post.username%>/"><%= post.username %></a> |
					日期：<%= post.time.minute %>
				</p>
				<p><%- post.post %></p>
			<% }) %>
		</div>
	</section>

</article>

<footer></footer>

